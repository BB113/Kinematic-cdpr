      <mass value="10000" />
      <inertia ixx="10000" ixy="0.0" ixz="0.0" iyy="10000" iyz="0.0" izz="10000" />
    </inertial>
  </link>

  <link name="frame_5">
    <collision>
      <origin xyz="${frame_lenght/2} ${frame_lenght/2} ${frame_lenght/2}" rpy="0 0 0" />
