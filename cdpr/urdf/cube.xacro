      <mass value="${platform_mass}" />
      <inertia ixx="${platform_iaa}" ixy="${platform_iab}" ixz="${platform_iab}" iyy="${platform_iaa}" iyz="${platform_iab}" izz="${platform_iaa}" />
    </inertial>
